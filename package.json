{
	"name": "momentumm",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
		"start": "DEBUG=express:* node ./bin/www"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"dependencies": {
		"axios": "^0.18.0",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.4",
		"debug": "~2.6.9",
		"express": "~4.16.0",
		"mongoose": "^5.2.9",
		"morgan": "~1.9.0",
		"vue": "^2.5.11",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.0",
		"babel-preset-stage-3": "^6.24.1",
		"cross-env": "^5.0.5",
		"css-loader": "^0.28.7",
		"eslint": "^5.4.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-vue": "^4.7.1",
		"file-loader": "^1.1.4",
		"husky": "^1.0.0-rc.13",
		"lint-staged": "^7.2.2",
		"node-sass": "^4.5.3",
		"prettier": "^1.14.2",
		"sass-loader": "^6.0.6",
		"vue-loader": "^13.0.5",
		"vue-template-compiler": "^2.4.4",
		"webpack": "^3.6.0",
		"webpack-dev-server": "^2.9.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,vue}": [
			"eslint"
		],
		"*.{js,css,md,vue}": [
			"prettier --write",
			"git add"
		]
	}
}
